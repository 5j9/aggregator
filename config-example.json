{
	"subscriptions": [
		{
			"url": "https://www.example.org/rss.aspx",
			"selector_lines": [
				"# Any python statement is allowed.",
				"# Just define links and titles.",
				"# See collector.py for available functions.",
				"parsed = parse('html', body)",
				"rows = parsed.cssselect('tr')[1:]",
				"links = []",
				"titles = []",
				"for row in rows:",
				"    try:",
				"        href = row.cssselect('td:last-child a')[0].attrib['href']",
				"    except IndexError:",
				"        continue",
				"    links.append(href)",
				"    titles.append(row.cssselect('.subdesc')[0].text)"
			]
		},
		{
			"ssl": false,
			"url": "https://example.com/",
			"selector_lines": [
				"xpath = parse('xml', body).xpath",
				"links = xpath('//item/link/text()')",
				"titles = xpath('//item/title/text()')"
			]
		}
	]
}